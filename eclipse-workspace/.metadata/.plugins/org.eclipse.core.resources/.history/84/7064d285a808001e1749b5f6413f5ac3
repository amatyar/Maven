package one;

public class ifelse {

	public static void main(String[] args) {
		int a = 128;
		
		if (a > 0  && a <= 1000) {
			System.out.println("You can get extra drink");
		}
		if (a> 10 && a<= 900)
		{
			System.out.println("Two more drink please");
			
		}
		if (a>100 && a<= 1001)
		{
			System.out.println("No more drink");
		}
	}
}
