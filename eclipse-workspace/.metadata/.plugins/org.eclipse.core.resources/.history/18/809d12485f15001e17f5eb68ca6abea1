}
class Student
{
	int id, year;
	String Name, contact;
	
	//public Student(int ID ) 
	//{
		//this.id= ID;	
		
	//}
	
	public Student(int ID, int Year, String Contact, String name) 
	{
		this.id= ID;
		this.year= Year;
		this.contact = Contact;
		this.Name = name;
	}
	
}



